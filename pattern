// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,k;
     for(int i=0;i<=4;i++)
     {    a=4,k=0;
          for(int j=0;j<=8;j++)
         {
             if(k>=(i+1))
             break;
         else if((i+j)==a)
         {
         printf("*");
         a=a+2;
         k++;
         }
         else
         printf(" ");
         
         }
         printf("\n");
     }
    return 0;
}
